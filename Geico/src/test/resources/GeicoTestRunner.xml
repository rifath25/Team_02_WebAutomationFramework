<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Geico Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
<!--    <parameter name="os" value="OS X"/>-->
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
<!--    <parameter name="os_version" value="Sierra"/>-->
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="77"/>
    <parameter name="url" value="https://www.Geico.com/"/>

    <test name = "Gieco HomePage">
        <classes>
            <class name="testLayer.TestHomePage"/>
            <class name="testLayer.TestReportClaimPage"/>
            <class name="testLayer.TestLoginPage"/>
            <class name="testLayer.TestClaimCenterPage"/>
            <class name="testLayer.TestAutoInsurancePage"/>
            <class name="testLayer.TestSearchPage"/>

        </classes>
    </test>


</suite>